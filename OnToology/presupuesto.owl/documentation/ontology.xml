        <dc:title xml:lang="es">Vocabulario para la representación de datos sobre presupuestos de entidades locales (incluyendo gastos e ingresos)</dc:title>
        <terms:hasFormat rdf:resource="http://vocab.linkeddata.es/datosabiertos/def/hacienda/presupuestos.html"/>
        <terms:hasFormat rdf:resource="http://vocab.linkeddata.es/datosabiertos/def/hacienda/presupuestos.owl"/>
